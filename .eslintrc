{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      0
    ],
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1
      }
    ],
    "camelcase": 0,
    "no-console": 0,
    "require-jsdoc": 0,
    "max-len": 0
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "es6": true
  }
}